{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "commands": [
        "cd frontend && npm install && npm run build"
      ]
    },    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  